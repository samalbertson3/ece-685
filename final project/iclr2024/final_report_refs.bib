	journal = {PloS One},
	author = {Lanata, Claudio F. and Fischer-Walker, Christa L. and Olascoaga, Ana C. and Torres, Carla X. and Aryee, Martin J. and Black, Robert E. and {Child Health Epidemiology Reference Group of the World Health Organization and UNICEF}},
	year = {2013},
