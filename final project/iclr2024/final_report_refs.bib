	keywords = {Child, Preschool, Diarrhea, Female, Global Health, Humans, Incidence, Male, Cross-Sectional Studies, Prognosis, Retrospective Studies, Rotavirus Infections, Rotavirus Vaccines, Survival Rate, Vaccination},
